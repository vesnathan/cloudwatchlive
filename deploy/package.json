{
  "name": "@cloudwatchlive/deploy",
  "version": "1.0.0",
  "description": "CloudWatch Live Deployment Scripts",
  "main": "deploy.ts",
  "license": "MIT",
  "scripts": {
    "deploy": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config deploy.ts",
    "deploy:dev": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config deploy.ts --stage dev",
    "deploy:prod": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config deploy.ts --stage prod",
    "remove-stacks": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config remove-stacks.ts --interactive",
    "remove-stacks:dev": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config remove-stacks.ts --stage dev --all",
    "remove-stacks:prod": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config remove-stacks.ts --stage prod --all"
  },
  "dependencies": {
    "@aws-sdk/client-appsync": "^3.821.0",
    "@aws-sdk/client-cloudformation": "^3.300.0",
    "@aws-sdk/client-cloudfront": "^3.817.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.817.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.817.0",
    "@aws-sdk/client-dynamodb": "^3.817.0",
    "@aws-sdk/client-lambda": "^3.817.0",
    "@aws-sdk/client-s3": "^3.300.0",
    "@aws-sdk/lib-dynamodb": "^3.817.0",
    "@types/archiver": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^2.1.4",
    "archiver": "^7.0.1",
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.20.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.2",
    "inquirer": "^8.2.4",
    "mime-types": "^3.0.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^8.2.4",
    "@types/node": "^18.11.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
